<component name="bss_populate" description="Connections BSS user population tool"
    project-type="java" spec-version="1.0">

    <output-group name="default">
        <output type="jar" visibility="public" platform="all"
                preferredInstallLocation="bin"
                location="${env.LWP_COMP_BUILD}/lib/bss_populate.jar" />
        <output type="directory" visibility="public" platform="all"
                preferredInstallLocation="bin"
                location="${env.LWP_COMP_BUILD}/bin" />
