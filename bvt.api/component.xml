<component name="bvt.api"
    description="Connections Data Population API BVT"
    project-type="java" spec-version="1.0">

    <output-group name="default">
        <output type="jar" visibility="public" platform="all"
            location="${env.LWP_COMP_BUILD}/lib/bvt.api.jar"
            preferredInstallLocation="/" />
        <output type="dir" visibility="public" platform="all"
        	location="${env.LWP_COMP_BUILD}/test_config"
            preferredInstallLocation="test_config"/>
        <output type="dir" visibility="public" platform="all"
            location="${env.LWP_COMP_BUILD}/resources"
            preferredInstallLocation="/resources"/>
    </output-group>

	<dependency name="libs.auto/wink-json4j" type="jar" version="1.1" />
	<dependency name="libs.auto/json4j" type="jar" version="1.0" />
	<dependency name="libs.auto/json-path" type="jar" version="0.5" />
    <dependency name="libs.auto/httpcore" type="jar" version="4.0" />
	<dependency name="libs.auto/httpclient" type="jar" version="4.0" />
	<dependency name="libs.auto/commons-io" type="jar" version="1.4" />
	<dependency name="libs.auto/junit" type="jar" version="4.5" />
	<dependency name="lc.data.pop" type="jar" version="1.0" />
	<dependency name="libs.auto/apache-abdera-1.1.2" type="jar" version="1.1.2" />
	<dependency name="libs.auto/testng" type="jar" version="6.4" />
	<dependency name="libs.auto/apache-ant-1.9.2" type="jar" version="1.9.2" />
	<dependency name="libs.auto/slf4j" type="jar" version="1.7" />
</component>
