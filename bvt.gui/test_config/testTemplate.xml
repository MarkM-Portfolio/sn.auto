      <classes>
      <class name="com.ibm.conn.auto.tests.homepage.BVT_Level_2_Homepage_AS" />
    </classes>
  </test>

  <test name="Forums" thread-count="2" parallel="methods" preserve-order="true" enabled="false">
    <packages>
      <package name="com.ibm.conn.auto.tests.forums" />
    </packages>
  </test>

  <test name="Gallery" parallel="tests" preserve-order="true" enabled="false">
    <classes>
      <class name="com.ibm.conn.auto.tests.communities.BVT_Level_2_Communities_Gallery" />
