  </test>

  <test name="Mentions" parallel="tests" preserve-order="true" enabled="false">
    <classes>
      <class name="com.ibm.conn.auto.tests.mentions.BVT_Homepage_Mentions" />
    </classes>
  </test>

  <test name="Profiles" parallel="tests" preserve-order="true" enabled="false">
    <packages>
      <package name="com.ibm.conn.auto.tests.profiles" />
    </packages>
  </test>

  <test name="Search" parallel="tests" preserve-order="true" enabled="false">
    <packages>
      <package name="com.ibm.conn.auto.tests.globalsearch" />
    </packages>
  </test>

  <test name="Sharebox" parallel="tests" preserve-order="true" enabled="false">
    <packages>
      <package name="com.ibm.conn.auto.tests.sharebox" />
    </packages>
  </test>

  <test name="Wikis" thread-count="3" parallel="methods" preserve-order="true" enabled="false">
    <packages>
      <package name="com.ibm.conn.auto.tests.wikis" />
    </packages>
  </test>
  
  <test name="Touchpoint" parallel="tests" preserve-order="true" enabled="false">
    <packages>
      <package name="com.ibm.conn.auto.tests.touchpoint" />
   </packages>
  </test>
  
  

