 <parameter name="server_is_grid_hub" value="true" />
  <parameter name="server_is_legacy_grid" value="false" />
  <parameter name="server_host" value="testautolb.cnx.cwp.pnp-hcl.com" />
  <parameter name="server_port" value="4444" />

