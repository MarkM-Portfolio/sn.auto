        <output type="jar" visibility="public" platform="all"
                location="${env.LWP_COMP_BUILD}/lib/lc.data.pop.web.jar"
                preferredInstallLocation="WEB-INF/lib" />
    </output-group>

    <dependency name="prereqs.sn/was7.0" type="jar" version="7.0" /> 
    <dependency name="lc.data.pop.adapter" type="jar" version="1.0" />
    <dependency name="lc.data.pop" type="jar" version="1.0" />
