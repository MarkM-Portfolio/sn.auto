<component name="lc.data.pop.war" project-type="war"
    description="Connections Data Population Web App"
    spec-version="1.0" disabled="false">

    <output-group name="to_ear">
        <output type="war" visibility="public" platform="all"
                location="${env.LWP_COMP_BUILD}/lib/lc.data.pop.war" />
