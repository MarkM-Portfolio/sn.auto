sub processLog {
  while (<LOGFILE>) {
    chomp;
    @matches = $_ =~ /","\d{4}\-(\d{2})\-(\d{2})","/g;
    $mmdd = $matches[0].$matches[1];
    if ($mmdd) {
      $dbh->do("INSERT DELAYED INTO `".$result->{'name'}.$mmdd."` VALUES (".$dbh->quote($hostname).",".$_.")");
    }
  }
  $lasteof = tell(LOGFILE);
}
