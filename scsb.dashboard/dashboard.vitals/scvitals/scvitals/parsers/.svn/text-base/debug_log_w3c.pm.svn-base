($junk,$junk,$junk,$dd,$mm,$junk,$junk,$junk,$junk) = localtime(time);
$mmdd = sprintf("%02d",$mm+1).sprintf("%02d",$dd);

sub processLog {
  while (<LOGFILE>) {
    chomp;
    $dbh->do("INSERT DELAYED INTO `".$result->{'name'}.$mmdd."` VALUES ($_)");
  }
  $lasteof = tell(LOGFILE);
}
