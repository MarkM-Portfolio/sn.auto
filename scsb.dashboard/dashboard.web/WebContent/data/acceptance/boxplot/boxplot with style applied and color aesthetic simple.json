{
  "data": [
    {
      "id": "dData",
      "fields": [
        {"id": "x", "unit": "categorical"},
        {"id": "y", "min": -0.5, "max": 7},
        {"id": "a"},
        {"id": "b"},
        {"id": "c"},
        {"id": "d"}
      ],
      "rows": [
        [ 1, 1, -0.5, 0, 2, 3 ]
	]
    }
  ],
  "copyright": "(C) Copyright IBM Corp. 2011",
  "grammar": [
    {
      "coordinates": {
        "dimensions": [
          {"axis": {"gridStyle": {"fill": "#ddf"}}},
          {"axis": {}}
        ]
      },
      "elements": [
        {
          "type": "schema:boxplot",
          "position": [
            {"field": {"$ref": "y"}},
            {"field": {"$ref": "x"}}
          ],
          "color": [
            {"field": {"$ref": "x"}},
            {
              "field": {"$ref": "x"},
              "modifies": {"part": "upperWhisker"}
            },
            {
              "field": {"$ref": "x"},
              "modifies": {"part": "lowerWhisker"}
            },
            {
              "field": {"$ref": "y"},
              "modifies": {"part": "median"}
            }
          ],
          "components": [
            {"field": {"$ref": "a"}},
            {"field": {"$ref": "b"}},
            {"field": {"$ref": "c"}},
            {"field": {"$ref": "d"}}
          ]      
        }
      ]
    }
  ],
  "version": "1.2"
}