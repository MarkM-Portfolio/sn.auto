{
 "data": [

    {
     "provider" :
      {
      "providerId" : "com.ibm.rave.CSVDataProvider",
      "args": [
	{
         "source": { "$ref" : "id_date"}
        },
         {"hasHeader" : true}
      ]
    }
  }
  ],
"resource" : [
     { 
         "id" : "id_date", 
        "type" : "data/csv/text", 
	"URIs" : ["/csv/Bar12-Date1.csv"]
     }
]
,  "copyright": "(C) Copyright IBM Corp. 2011",
  "grammar": [
    {
      "coordinates": {
        "dimensions": [
          {
            "axis": {
              "minorStyle": {
                "fill": {"b": 75, "g": 75, "r": 75}
              },
              "title": [ "Count" ],
              "titleStyle": {
                "fill": {"b": 150, "g": 50, "r": 50},
                "font": {"size": "18", "weight": "bold"}
              }
            }
          },
          {
            "scale": {"local": true},
            "axis": {
              "markStyle": {
                "fill": {"b": 0, "g": 0, "r": 0}
              },
              "tickStyle": {
                "fill": {"b": 0, "g": 0, "r": 0},
                "font": {
                  "family": "Verdana,SanSerif",
                  "size": 11.1
                }
              }
            }
          }
        ]
      },
      "elements": [
        {
          "type": "interval",
          "position": [
            {"field": {"$ref": "Values"}},
            {"field": {"$ref": "Dates"}}
          ],
          "color": [ {"field": {"$ref": "Values"}} ],
          "tooltip": [
            {
              "content": [
                {"$ref": "Dates"},
                " \n ",
                {"$ref": "Values"}
              ]
            }
          ],
          "style": {"width": "80%", "outline": {"a": 0.0}}
        }
      ],
      "labelCollisionMethod": "none",
      "style": {"fill": "white", "padding": 5}
    }
  ],
  "version": "4.0"
}
