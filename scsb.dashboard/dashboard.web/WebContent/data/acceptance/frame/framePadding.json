{
  "copyright": "(C) Copyright IBM Corp. 2013",
  "grammar": [
    {
      "bounds": {"width": 200, "height": 100, "left": "50%"},
      "coordinates": {
      },
      "elements": [
        {
          "type": "interval",
          "label": [ {"content": [ "padding=5%" ]} ],
          "style": {
            "fill": {"a": 1.0, "b": 0, "g": 0, "r": 255}
          }
        }
      ],
      "style": {
        "fill": "white",
        "stroke": {"width": 5},
        "outline": "cyan",
        "padding": "5%"
      }
    },
    {
      "bounds": {
        "width": 200,
        "height": 100,
        "right": "50%"
      },
      "coordinates": {
      },
      "elements": [
        {
          "type": "interval",
          "label": [ {"content": [ "padding={top=5%, left=5%, right=5%, bottom=5%" ]} ],
          "style": {
            "fill": {"a": 1.0, "b": 0, "g": 0, "r": 255}
          }
        }
      ],
      "style": {
        "fill": "white",
        "stroke": {"width": 5},
        "outline": "cyan",
        "padding": {
          "bottom": "5%",
          "left": "5%",
          "right": "5%",
          "top": "5%"
        }
      }
    }
  ],
  "version": "1.3",
  "style": {
    "fill": "yellow",
    "stroke": {"width": 10},
    "cornerRadius": 0,
    "outline": "black",
    "outlineMethod": "inside",
    "padding": 20
  }
}