{
  "data": [
    {
      "id": "links",
      "fields": [
        {"id": "from"},
        {"id": "to"},
        {"id": "size"},
        {
          "id": "direction",
          "categories": [ "oneway", "twoway" ]
        }
      ],
      "rows": [
        [ 3, 0, 0, 0 ],
        [ 1, 0, 0, 0 ],
        [ 4, 1, 0, 0 ],
        [ 4, 2, 0, 0 ],
        [ 5, 4, 0, 0 ],
        [ 6, 4, 0, 0 ],
        [ 7, 3, 0, 0 ],
        [ 7, 4, 0, 0 ],
        [ 4, 3, 0, 0 ]
      ]
    },
    {
      "id": "nodes",
      "fields": [
        {"id": "id"},
        {
          "id": "name",
          "categories": [
            "Node0",
            "Node1",
            "Node2",
            "Node3",
            "Node4",
            "Node5",
            "Node6",
            "Node7"
          ]
        },
        {
          "id": "layer",
          "categories": [
            "Layer3",
            "Layer2",
            "Layer1",
            "Layer0"
          ]
        },
        {
          "id": "status",
          "categories": [ "good", "soso", "bad", "invalid" ]
        }
      ],
      "rows": [
        [ 0, 0, 3, 2 ],
        [ 1, 1, 2, 1 ],
        [ 2, 2, 2, 2 ],
        [ 3, 3, 2, 0 ],
        [ 4, 4, 1, 1 ],
        [ 5, 5, 0, 0 ],
        [ 6, 6, 0, 0 ],
        [ 7, 7, 0, 1 ]
      ]
    }
  ],
  "copyright": "(C) Copyright IBM Corp. 2012",
  "grammar": [
    {
      "coordinates": {
        "dimensions": [ {}, {} ]
      },
      "elements": [
        {
          "type": "point",
          "label": [
            {
              "content": [ {"$ref": "name"} ],
              "modifies": {"part": "label"},
              "style": {"location": "inside"}
            }
          ],
          "data": {"$ref": "nodes"},
          "color": [
            {
              "field": {"$ref": "status"},
              "mapping": [
                {"at": 0, "color": "#b4cd3d"},
                {"at": 1, "color": "#fad641"},
                {"at": 2, "color": "#f37c8a"},
                {
                  "at": 3,
                  "color": {
                    "a": 0.6,
                    "b": 128,
                    "g": 128,
                    "r": 128
                  }
                }
              ],
              "modifies": {"type": "fill", "part": "main"}
            }
          ],
          "positioning": {
            "id": {"$ref": "id"},
            "from": {"$ref": "from"},
            "layer": {"$ref": "layer"},
            "layerPlacement": "centered",
            "method": "dag",
            "nodePlacement": "maxOverlap",
            "orientation": "top-to-bottom",
            "to": {"$ref": "to"}
          },
          "style": {
            "width": 150,
            "height": 30,
            "symbol": {
              "shape": [
                {
                  "x": "0.0%",
                  "y": "0.0%",
                  "width": "100%",
                  "height": "100%",
                  "part": "main",
                  "style": {"stroke": {"width": 1}}
                },
                {
                  "x": "0.0%",
                  "y": "0.0%",
                  "width": "100%",
                  "height": "100%",
                  "part": "label",
                  "style": {"fill": "transparent"}
                }
              ]
            }
          }
        },
        {
          "type": "edge",
          "data": {"$ref": "links"},
          "positioning": {
            "id": {"$ref": "id"},
            "attachmentPoint": "center",
            "from": {"$ref": "from"},
            "method": "link",
            "to": {"$ref": "to"}
          },
          "size": [
            {
              "field": {"$ref": "size"},
              "mapping": [
                {"at": "0.0%", "size": 100},
                {"at": "100.0%", "size": 150},
                {"at": "200.0%", "size": 200},
                {"at": "300.0%", "size": 250}
              ]
            }
          ],
          "styleBy": [
            {
              "field": {"$ref": "direction"},
              "palette": [
                {"endArrow": "arrow"},
                {
                  "endArrow": "arrow",
                  "startArrow": "circle"
                }
              ]
            }
          ],
          "style": {
            "fill": {"a": 1.0, "b": 130, "g": 130, "r": 130},
            "stroke": {"width": 3.0, "cap": "butt"},
            "symbol": "curved"
          }
        }
      ],
      "style": {
        "fill": "#FFFFFF",
        "outline": {"a": 0.1},
        "padding": 15
      }
    }
  ],

  "version": "1.3",
  "style": {"fill": "#DEDEDE", "padding": 5}
}