{
  "data": [
    {
      "id": "links",
      "fields": [
        {"id": "from"},
        {"id": "to"},
        {"id": "direction", "categories": [ "oneway", "twoway", "backwards" ]}
      ],
      "rows": [
        [ 3, 0, 0 ],
        [ 4, 1, 0 ],
        [ 4, 2, 0 ],
        [ 5, 3, 0 ],
        [ 6, 5, 0 ],
        [ 7, 2, 0 ],
        [ 7, 4, 1 ],
        [ 0, 2, 0 ],
        [ 0, 1, 0 ],
        [ 1, 0, 0 ],
        [ 2, 1, 0 ],
        [ 2, 0, 0 ],
        [ 4, 3, 0 ],
        [ 4, 5, 0 ],
        [ 7, 3, 0 ]
      ]
    },
    {
      "id": "nodes",
      "fields": [
        {"id": "name", "categories": [ "0", "1", "2", "3", "4", "5", "6", "7" ]},
        {"id": "layer", "categories": [ "1", "2", "3", "4" ]}
      ],
      "rows": [
        [ 0, 3 ],
        [ 1, 3 ],
        [ 2, 3 ],
        [ 3, 2 ],
        [ 4, 2 ],
        [ 5, 2 ],
        [ 6, 0 ],
        [ 7, 0 ]
      ]
    }
  ],
  "copyright": "(C) Copyright IBM Corp. 2011",
  "grammar": [
    {
      "coordinates": {
        "dimensions": [ {}, {} ]
      },
      "elements": [
        {
          "type": "point",
          "label": [ {"content": [ {"$ref": "name"} ]} ],
          "data": {"$ref": "nodes"},
          "positioning": {
            "id": {"$ref": "name"},
            "from": {"$ref": "from"},
            "layer": {"$ref": "layer"},
            "layerPlacement": "centered",
            "method": "dag",
            "nodePlacement": "symmetric",
            "to": {"$ref": "to"}
          },
          "style": {"width": 50, "height": 20, "symbol": "rectangle"}
        },
        {
          "type": "edge",
          "data": {"$ref": "links"},
          "positioning": {
            "id": {"$ref": "name"},
            "from": {"$ref": "from"},
            "method": "link",
            "to": {"$ref": "to"}
          },
          "styleBy": [
            {
              "field": {"$ref": "direction"},
              "palette": [
                {"endArrow": "arrow"},
                {"endArrow": "arrow", "startArrow": "arrow"},
                {"startArrow": "arrow"}
              ]
            }
          ],
          "style": {
            "fill": {"a": 0.1},
            "stroke": {"width": 2.5},
            "endArrow": "arrow",
            "symbol": "elbow"
          }
        }
      ]
    }
  ],
  "version": "1.3"
}